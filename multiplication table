namespace ConsoleApp1
{
    internal class Program
    {
        static void Main(string[] args)
        {
            Console.Write("Enter a number to multiply: ");
            int number = Convert.ToInt32(Console.ReadLine());
            int a;

            for (int i = 1; i < 11; i++)
            {
                a = number * i;
                Console.WriteLine($"{number} * {i} = {a}");
            }
        }
    }
}
